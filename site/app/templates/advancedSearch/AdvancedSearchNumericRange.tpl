<div class='col-lg-4 col-md-4 col-sm-6 col-xs-12 orcaAdvancedNumericRangeField'>
	<div class='form-group clearfix'>
		<label for='column_{{ COLUMN }}' class='control-label'>{{ TITLE }}</label>
		<div class='col-lg-6 col-md-6 col-sm-6 col-xs-12'>
			<input type='text' class='form-control' id='column_{{ COLUMN }}_MIN' name='column_{{ COLUMN }}_MIN' placeholder='Minimum Value' />
		</div>
		<div class='col-lg-6 col-md-6 col-sm-6 col-xs-12'>
			<input type='text' class='form-control' id='column_{{ COLUMN }}_MAX' name='column_{{ COLUMN }}_MAX' placeholder='Maximum Value' />
		</div>
	</div>
</div>